<?xml version="1.0" encoding="utf-8"?>
<?pte APIVersion="0.0.02"?>
<package type="transaction" name="Vertragsbestätigung" description="Eine Beschreibung zur Vertragsbestätigung" version="3.1">
  <configurationSet>
    <runtimeEnvironments>
      <runtimeEnvironment platform="Windows">
                    <program name="bn-RW" version="5.09.*" />
                    <cmdLine>blubber</cmdLine>
                    <programResult>
                        <log>*.log,*.ext,*.xtf,*.docref,*.sapref</log>
                        <preview>*.pdf,*.xml</preview>
                        <result type="ReturnCode">0</result>
                    </programResult>
                </runtimeEnvironment><runtimeEnvironment platform="Linux">
                    <program name="emaks-RW" version="5.10.*" />
                    <cmdLine></cmdLine>
                    <programResult>
                        <log>*.log,*.ext,*.xtf,*.docref,*.sapref</log>
                        <preview>*.pdf,*.xml</preview>
                        <result type="ReturnCode">0</result>
                    </programResult>
                </runtimeEnvironment>
    </runtimeEnvironments>
    <transactionForm>${home}/./forms/ZABSCHLAGS_AEND.xdp</transactionForm>
    <transactionFormPageBackgrounds>
      <transactionFormPageBackground pageName="emaks_first_page">./backgrounds/emaks_first_page.png</transactionFormPageBackground>
<transactionFormPageBackground pageName="emaks_second_page">./backgrounds/emaks_second_page.png</transactionFormPageBackground>

    </transactionFormPageBackgrounds>
    <fontDef>
      <location>${home}etc/fonts.ini</location>
    </fontDef>
    <transactionFormMaterials>
      <material name="A4" description="StandardA4" width="210mm" height="297mm" thickness="0.1mm" weight="4.99g" />
<material name="Letter" description="Letter" width="210mm" height="297mm" thickness="0.1mm" weight="4.99g" />

    </transactionFormMaterials>
    <testDataSet>
      <testData><name>Abschlagsaenderung</name><description>2 Dokumente</description><value>testdata/20220307_abschlagsaenderung.prt</value></testData>
<testData><name>Abschlagsaenderung</name><description>2 Dokumente ein zweites mal</description><value>testdata/20220307_abschlagsaenderung.prt</value></testData>
<testData><name>Abschlagsaenderung</name><description>2 Dokumente ein zweites mal</description><value>testdata/20220307_abschlagsaenderung.prt</value></testData>

    </testDataSet>
    <inputVariableSet>
      <inputVariable><name>GD_DATA_LABEL</name><description>Some data for the RS</description></inputVariable>
<inputVariable><name>GD_DATA_SPARTE</name><description>Sparte for the RS</description></inputVariable>

    </inputVariableSet>
  </configurationSet>
  <supplement>
    <logicalSupplement>
      <allowed>True</allowed>
      <useOnlyTransactionFormPaper>True</useOnlyTransactionFormPaper>
    </logicalSupplement>
    <physicalSupplement>
      <allowed>False</allowed>
    </physicalSupplement>
  </supplement>
  <shipment>
    <postage>
      <optionalSupplementCanExceedPostage>True</optionalSupplementCanExceedPostage>
      <whitespaceCanExceedPostage>False</whitespaceCanExceedPostage>
    </postage>
  </shipment>
  <whitespace>
    <allowed>True</allowed>
    <maxSpace>50</maxSpace>
    <overflow>ALLOWTOCREATEBACKPAGE</overflow>
    <partSet />
  </whitespace>
</package>
